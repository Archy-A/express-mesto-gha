{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
      "plugin:react/recommended",
      "airbnb-base",
      "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "import"
    ],
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }]
    }
}
